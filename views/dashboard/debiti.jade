script
  function compute() {
    mb = new MoneyBook();
    mb.parse($("#inputfile").val());
    mb.computeSummary();
    mb.computeSummaryByDate();
    drawChart(mb, "chart_div");
    $("#charts_div").html("");
    drawChartsByDay(mb, "charts_div");
  }
  function load_url() {
    $('#inputfile').val(money);
    $('#inputfile').scrollTop(9999);
    compute();
  }
  $(document).ready(function() {
     load_url();
   });

